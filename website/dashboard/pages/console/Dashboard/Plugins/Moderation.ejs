                <div class="col-xl-8 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="row widget-statistic">
                        <div class="col-xl-4 col-lg-4 col-md-4 col-sm-4 col-12 layout-spacing">
                            <div class="widget widget-one_hybrid widget-referral">
                                <div class="widget-heading">
                                    <div class="w-title">
                                       
                                        <div class="">
                            

                                            <script>
                                                function run() {
                                                    if (db.get('main', 'Moderation_'+req.params.id) == "false") {
                                                    Swal.fire({
                                                        title: "Enable moderation?",
                                                         text: "You don't have moderation enabled! To use this plugin enable moderation!",
                                                          icon: "error",
                                                           closeOnConfirm: false,
                                                            footer: 'Copyright 2021 &copy Nova bot™ - All right reserved.',  
                                                            confirmButtonText: 'Enable!',
                                                           denyButtonText: `Don't enable`,
                                                           showDenyButton: true,
                                                        }).then((result) => {
  /* Read more about isConfirmed, isDenied below */
  if (result.isConfirmed) {
    Swal.fire({title: "Success!",
                                                         text: "Moderation enabled!",
                                                          icon: "success",
                                                           closeOnConfirm: true,
                                                            footer: 'Copyright 2021 &copy Nova bot™ - All right reserved.'})
  } else if (result.isDenied) {
    Swal.fire({title: "Success!",
                                                         text: "Not enabled!",
                                                          icon: "info",
                                                           closeOnConfirm: true,
                                                            footer: 'Copyright 2021 &copy Nova bot™ - All right reserved.'})
                                                            window.location.replace(config.website_settings.domain+'/console/dashboard/'+req.params.id);

                                                        }
})


                                                    }

                                            }
                                              
                                            </script>
                                            <p class="w-value">AutoMod</p>
                                            <h5 class=""><%if (db.get('main', 'antiSwear_'+req.params.id) == false ) { %>
                                                AntiSwear : Disabled 
                                                <% }else{ %> 
                                                    AntiSwear : Enabled
                                                    <% } %></h5></br>

                                                <h5 class=""><%if (db.get('main', 'antispam_'+req.params.id) == false){%>
                                                    AntiSpam : Disabled
                                                    <%} else {%>
                                                    AntiSpam : Enabled
                                                    <%}%></h5></br>     
                                                    
                                                     <h5 class=""><%console.log(db.get('main', 'AntiCap_'+req.params.id)); if (db.get('main', 'AntiCap_'+req.params.id).value == false){%>
                                                AntiCaps : Disabled
                                                <%} else {%>
                                                AntiCaps : Enabled
                                                <%}%></h5></br>

                                                 <h5 class=""><%if (db.get('main', 'antilink_'+req.params.id) == false){%>
                                                AntiLink : Enabled
                                                <%} else {%>
                                                AntiLink : Disabled
                                                <%}%></h5></br>
                                        </div>
